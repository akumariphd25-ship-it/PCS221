   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "List after swapping: [10, 40, 30, 20, 50]\n"
     ]
    }
   ],
   "source": [
    "# Positions to swap\n",
    "pos1 = 1\n",
    "pos2 = 3\n",
    "\n",
    "# Using temporary variable\n",
    "temp = my_list[pos1]\n",
    "my_list[pos1] = my_list[pos2]\n",
    "my_list[pos2] = temp\n",
    "\n",
    "print(\"List after swapping:\", my_list)"
   ]
